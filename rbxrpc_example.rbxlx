				<ProtectedString name="Source"><![CDATA[return function(api)
	local client = {}
	local server = {}
	local common = {}
	
	local serverOnly = {}
	local clientOnly = {}
	
	function client.foo(x)
		print("CLIENT: foo called from server with", x)
		return api.bar(10)
	end
	
	function server.bar(caller, x)
		print("SERVER: bar called from", caller, "with", x)
		return 2
	end
	
	function common.baz()
		return 3
	end
	
	function clientOnly.quz(x)
		print("CLIENT: quz called with", x)
		return 4
	end
	
	function serverOnly.quz(x)
		print("SERVER: quz called with", x)
		return 5
	end
	
	return {
		name = "Testerino", 
		client = client, 
		server = server, 
		common = common,
		serverOnly = serverOnly,
		clientOnly = clientOnly,
	}
end]]></ProtectedString>
